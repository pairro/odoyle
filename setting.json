      "position":"start"
    },
    {
      "name-element":"meta",
      "data-attribute":[
        {
          "name-attribute":"property",
          "value-attribute":"article:author"
        },
        {
          "name-attribute":"content",
          "value-attribute":"${authorPost}"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    },
	 {
      "name-element":"script",
      "data-attribute":[
        {
          "name-attribute":"src",
          "value-attribute":"https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js"
        }
      ],
      "data-innerHTML":"",
      "position":"start"
    }
  ],
  "inject-element-body":[
    {
      "name-element":"script",
      "data-attribute":[
